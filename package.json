{
  "name": "iso-box-reader",
  "version": "1.0.0",
  "description": "A lightweight TypeScript parser for ISO Base Media File Format (ISO BMFF / fMP4) containers",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "iso",
    "bmff",
    "mp4",
    "fmp4",
    "parser",
    "media",
    "box"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/user/iso-box-reader.git"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}